  "graduation_and_promotion": {
    "future_ready_course_of_study": {
      "english": {
        "credits_required": 4,
        "description": "English I, English II, English III or AP English Language and Composition, and English IV or AP English Language and Composition."
      },
      "math": {
        "credits_required": 4,
        "description": "NC Math 1, NC Math 2, NC Math 3, and a fourth-level math course aligned with the student's post-secondary plans."
      },
      "science": {
        "credits_required": 3,
        "description": "Earth/Environmental Science, Biology, and a physical science course such as Chemistry or Physical Science."
      },
      "social_studies": {
        "credits_required": 4,
        "description": "World History or AP World History, Founding Principles of the US/NC: Civic Literacy or AP US Government & Politics, American History or AP US History, and Economics & Personal Finance."
      },
      "healthful_living": {
        "credits_required": 1,
        "description": "Healthful Living I (Honors at GLHS) and successful completion of the state CPR requirement."
      },
      "electives": {
        "credits_required": 6,
        "description": "At least two elective credits must be from a combination of Career and Technical Education (CTE), Arts, and World Languages. Beginning with students entering 9th grade in 2025-26, at least one Arts class in grades 6–12 is required."
      },
      "special_populations_reference": "For Occupational Course of Study or Extended Content Standards requirements, refer to the WCPSS High School Program Planning Guide (wcpss.net/high-school)."
    },
    "promotion_requirements": {
      "9_to_10": {
        "minimum_credits": 6,
        "required_components": [
          "English I",
          "2 credits in any combination of math, social studies, or science",
          "3 additional credits in any area"
        ]
      },
      "10_to_11": {
        "minimum_credits": 12,
        "required_components": [
          "English II",
          "1 math credit",
          "1 social studies credit",
          "1 science credit",
          "2 additional credits"
        ]
      },
      "11_to_12": {
        "minimum_credits": 18,
        "required_components": [
          "English III or AP English Language and Composition",
          "Enrolled in a program of study that, if completed, will meet graduation requirements"
        ]
      }
    },
    "alternate_graduation_pathways": {
      "three_year_graduation": {
        "description": "Application-based option allowing graduation with 22 credits after three years of high school (6 semesters) per North Carolina House Bill 259.",
        "typical_uses": [
          "Immediate entry into the workforce.",
          "Early enrollment at post-secondary institutions.",
          "Gap year plans."
        ],
        "contact": "Dean of Students (currently listed as Mrs. Hamilton Roberts)."
      },
      "mid_year_graduation": {
        "description": "Application-based option allowing graduation at mid-year of senior year (after 7 total semesters) for students who have met all graduation requirements.",
        "typical_uses": [
          "Early work or college start.",
          "Other postsecondary planning."
        ],
        "contact": "Dean of Students (currently listed as Mrs. Hamilton Roberts)."
      }
    }
  },
  "academics_structure": {
    "course_rigor_levels": {
      "academic": {
        "label": "Academic",
        "description": "Standard level courses with meaningful and rigorous work plus supports to help students succeed.",
        "weighting_note": "No additional quality point weighting beyond standard."
      },
      "honors": {
        "label": "Honors",
        "description": "More rigorous than Academic with deeper exploration of content; helps prepare students for AP courses.",
        "weighting_note": "Typically carries an additional 0.5 quality points."
      },
      "ap": {
        "label": "Advanced Placement (AP)",
        "description": "Most challenging courses; students may earn college credit by performing well on AP exams in May.",
        "weighting_note": "Typically carries an additional 1.0 quality points."
      }
    },
    "gpa_and_class_rank": {
      "gpa": {
        "unweighted": {
          "description": "Calculated by adding all quality points for courses in grades 9–12 and dividing by the total number of courses attempted, using the base (Academic) scale.",
          "includes": "All courses with standard letter grades A–F.",
          "excludes": "Courses graded as Pass (P) and credits earned via Credit by Demonstrated Mastery (CDM)."
        },
        "weighted": {
          "description": "Same calculation as unweighted, but includes extra quality points for Honors, AP, and dual-enrollment courses.",
          "weighting": {
            "honors_extra_points": 0.5,
            "ap_or_equivalent_extra_points": 1.0
          }
        },
        "quality_points_table": {
          "A": {
            "academic": 4.0,
            "honors": 4.5,
            "ap": 5.0
          },
          "B": {
            "academic": 3.0,
            "honors": 3.5,
            "ap": 4.0
          },
          "C": {
            "academic": 2.0,
            "honors": 2.5,
            "ap": 3.0
          },
          "D": {
            "academic": 1.0,
            "honors": 1.5,
            "ap": 2.0
          },
          "F": {
            "academic": 0.0,
            "honors": 0.0,
            "ap": 0.0
          },
          "letter_grade_scale": "A = 90–100; B = 80–89; C = 70–79; D = 60–69; F = 59 and below."
        }
      },
      "class_rank": {
        "calculation": "Total quality points divided by total credits attempted; rounded to four decimal places. Rankings are recalculated three times per year.",
        "weighted_courses": [
          "AP courses (and equivalent community college/university courses) carry +1.0 quality point.",
          "Honors courses carry +0.5 quality point."
        ]
      },
      "latin_honors": {
        "cum_laude": {
          "weighted_gpa_range": "3.75–3.99"
        },
        "magna_cum_laude": {
          "weighted_gpa_range": "4.00–4.249"
        },
        "summa_cum_laude": {
          "weighted_gpa_range": "4.25 or higher"
        }
      }
    }
  },
  "advising_guidance": {
    "general_course_selection_principles": [
      "Base course requests on individual interests, goals, and skill sets, not on friends' or siblings' choices.",
      "Consider teacher recommendations, current performance, and outside commitments.",
      "All requested courses (including alternates) may appear on the schedule."
    ],
