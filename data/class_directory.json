    "esl_guidance": {
      "rising_9": "Students who require ESL based on their LEP plan should work with their middle school counselor and ESL teacher to select ESL courses.",
      "rising_10_12": "Students with ESL services will work with their ESL case manager to request appropriate courses; schedules may be adjusted later if LEP plans change."
    },
    "special_education_guidance": {
      "rising_9": "Students with IEPs needing special education services (e.g., Curriculum Assistance) should coordinate course selection with their middle school counselor and IEP case manager.",
      "rising_10_12": "Students work with their case manager to select special education classes; schedules can be updated to reflect IEP changes."
    },
    "world_language_guidance": {
      "general": [
        "Students should only select one language path at a time: French OR Latin OR Spanish.",
        "Students wishing to place into a higher-level world language should follow placement information on the Green Level HS website."
      ],
      "rising_9_specific": [
        "Students who took a high school-level foreign language in middle school are encouraged to continue the same language in 9th grade to avoid gaps.",
        "Students who have not started a for-credit language before 9th grade may consider beginning in 10th grade."
      ]
    },
    "unique_opportunities_10_12": {
      "dual_enrollment_wake_tech_ccp": {
        "description": "Eligible students may take Wake Tech Community College courses for free through Career & Technical Education (CTE) or College Transfer pathways.",
